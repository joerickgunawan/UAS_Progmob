<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DataDosen">

    <EditText
        android:id="@+id/editTxtFotoDsn"
        android:layout_width="122dp"
        android:layout_height="40dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toStartOf="@+id/button4"
        app:layout_constraintTop_toBottomOf="@+id/editTxtGelarDsn" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="45dp"
        android:layout_height="22dp"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="38dp"
        android:text="Foto"
        app:layout_constraintEnd_toStartOf="@+id/editTxtFotoDsn"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <EditText
        android:id="@+id/editTxtGelarDsn"
        android:layout_width="211dp"
        android:layout_height="41dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="101dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTxtEmailDsn" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="45dp"
        android:layout_height="22dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="38dp"
        android:text="Gelar"
        app:layout_constraintEnd_toStartOf="@+id/editTxtGelarDsn"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <EditText
        android:id="@+id/editTxtEmailDsn"
        android:layout_width="211dp"
        android:layout_height="41dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="101dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTxtAlamatDsn" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="45dp"
        android:layout_height="22dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="38dp"
        android:text="E-mail"
        app:layout_constraintEnd_toStartOf="@+id/editTxtEmailDsn"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <EditText
        android:id="@+id/editTxtAlamatDsn"
        android:layout_width="211dp"
        android:layout_height="41dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="100dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTxtNidnDsn" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="55dp"
        android:layout_height="23dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="28dp"
        android:text="Alamat"
        app:layout_constraintEnd_toStartOf="@+id/editTxtAlamatDsn"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <EditText
        android:id="@+id/editTxtNidnDsn"
        android:layout_width="211dp"
        android:layout_height="41dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="101dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTxtNamaDsn" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="45dp"
        android:layout_height="22dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="38dp"
        android:text="NIDN"
        app:layout_constraintEnd_toStartOf="@+id/editTxtNidnDsn"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="45dp"
        android:layout_height="22dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="38dp"
        android:text="Nama"
        app:layout_constraintEnd_toStartOf="@+id/editTxtNamaDsn"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTxtNamaDsn"
        android:layout_width="211dp"
        android:layout_height="41dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="101dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button4"
        android:layout_width="89dp"
        android:layout_height="35dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="101dp"
        android:text="Browse.."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTxtGelarDsn" />

    <Button
        android:id="@+id/btnUploadImg"
        android:layout_width="290dp"
        android:layout_height="39dp"
        android:layout_marginStart="61dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="132dp"
        android:text="UPLOAD"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="117dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="167dp"
        android:layout_marginBottom="57dp"
        app:layout_constraintBottom_toTopOf="@+id/btnUploadImg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTxtFotoDsn"
        tools:srcCompat="@tools:sample/avatars" />
</androidx.constraintlayout.widget.ConstraintLayout>